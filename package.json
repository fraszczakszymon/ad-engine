		"@types/chai": "4.1.7",
		"@types/doubleclick-gpt": "238.0.1",
		"@types/jquery": "3.3.30",
		"@types/js-cookie": "2.2.2",
		"@types/lodash": "4.14.134",
		"@types/mocha": "5.2.7",
		"@types/sinon": "7.0.13",
		"@wdio/cli": "5.7.10",
		"@wdio/devtools-service": "5.7.8",
		"@wdio/local-runner": "5.7.9",
		"@wdio/mocha-framework": "5.7.8",
		"@wdio/selenium-standalone-service": "5.7.8",
		"@wdio/spec-reporter": "5.7.9",
		"@wdio/sync": "5.7.9",
		"allure-commandline": "2.7.0",
		"awesome-typescript-loader": "5.2.1",
		"babel-loader": "8.0.6",
		"babel-plugin-lodash": "^3.3.2",
		"babel-plugin-syntax-trailing-function-commas": "6.22.0",
		"chai": "^4.1.2",
		"chrome-remote-interface": "0.26.1",
		"concat": "1.0.3",
		"copy-webpack-plugin": "5.0.3",
		"deepmerge": "2.2.1",
		"express": "^4.16.2",
		"husky": "2.5.0",
		"js-md5": "0.7.3",
		"jsdom": "11.11.0",
		"jsdom-global": "^3.0.2",
		"json-loader": "0.5.7",
		"jwplayer-fandom": "github:wikia/jwplayer-fandom#1.6.1",
		"lint-staged": "8.2.1",
		"mini-css-extract-plugin": "0.7.0",
