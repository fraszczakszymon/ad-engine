{
	"name": "@wikia/ad-engine",
	"sideEffects": [
		"*.scss"
	],
	"version": "36.0.2",
	"description": "Wikia AdEngine",
	"repository": {
		"type": "git",
		"url": "https://github.com/Wikia/AdEngine"
	},
