{
	"name": "@wikia/ad-engine",
	"version": "25.2.0",
	"description": "Wikia AdEngine",
	"repository": {
		"type": "git",
		"url": "https://github.com/Wikia/AdEngine"
	},
	"dependencies": {
		"@babel/runtime-corejs2": "7.3.1",
		"blockadblock": "3.2.1",
		"core-decorators": "^0.20.0",
		"current-device": "0.7.8",
		"eventemitter3": "3.0.1",
		"js-cookie": "2.2.0",
		"lodash": "^4.17.4"
	},
	"devDependencies": {
		"@alugha/ima": "1.1.2",
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"@babel/plugin-proposal-decorators": "^7.0.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-typescript": "7.1.0",
		"@babel/register": "^7.0.0",
		"@slack/client": "4.1.0",
		"@types/chai": "4.1.7",
		"@types/mocha": "5.2.6",
		"@types/sinon": "7.0.7",
		"allure-commandline": "2.7.0",
		"babel-loader": "^8.0.0",
		"babel-plugin-lodash": "^3.3.2",
		"babel-plugin-syntax-trailing-function-commas": "6.22.0",
		"chai": "^4.1.2",
		"chrome-remote-interface": "0.26.1",
		"concat": "1.0.3",
		"copy-webpack-plugin": "4.5.2",
		"deepmerge": "2.2.1",
		"husky": "1.1.4",
		"js-md5": "0.7.3",
		"jsdom": "11.11.0",
		"jsdom-global": "^3.0.2",
		"json-loader": "0.5.7",
		"jwplayer-fandom": "github:wikia/jwplayer-fandom#1.6.1",
		"lint-staged": "8.0.5",
		"mini-css-extract-plugin": "0.4.0",
		"mocha": "4.1.0",
		"mocha-jsdom": "^1.1.0",
		"mocha-junit-reporter": "^1.15.0",
		"mocha-webpack": "2.0.0-beta.0",
		"node-sass": "4.9.3",
		"nyc": "11.4.1",
		"phantomjs-polyfill-object-assign": "0.0.2",
		"phantomjs-prebuilt": "^2.1.7",
		"portfinder": "1.0.20",
		"prettier": "1.16.4",
		"sass-loader": "^6.0.6",
		"selenium-standalone": "6.15.2",
		"sinon": "4.1.4",
		"string-replace-webpack-plugin": "0.1.3",
		"tslint": "5.12.1",
		"tslint-config-airbnb": "5.11.1",
		"tslint-config-prettier": "1.18.0",
		"tslint-eslint-rules": "5.4.0",
		"typescript": "3.3.3",
		"wdio-allure-reporter": "0.7.0",
		"wdio-devtools-service": "0.1.5",
		"wdio-dot-reporter": "0.0.10",
		"wdio-mocha-framework": "0.6.3",
		"wdio-screenshots-cleanup-service": "0.0.7",
		"wdio-selenium-standalone-service": "0.0.10",
		"wdio-static-server-service": "1.0.1",
		"wdio-visual-regression-service": "0.9.0",
		"webdriverio": "4.13.2",
		"webpack": "4.5.0",
		"webpack-cli": "2.0.14",
		"webpack-dev-server": "3.1.14",
		"webpack-merge": "4.1.2"
	},
	"files": [
		"dist/",
		"lib/",
		"maintenance/",
		"src/",
		".babelrc",
		".npmrc",
		"jsconfig.json",
		"README.md",
		"webpack.config.js"
	],
	"main": "./dist/ad-engine.js",
	"nyc": {
		"report-dir": "./spec/build/",
		"reporter": [
			"cobertura"
		]
	},
	"scripts": {
		"build": "webpack --progress --colors --hide-modules && node ./maintenance/build-bundle.js",
		"ci-check": "npm run test && npm run lint",
		"dev": "npm run serve",
		"lint": "tslint --project . && npm run rules:lint && ./maintenance/detect-incorrect-usages.sh",
		"lintf": "tslint --project . --fix && npm run rules:lintf &&  ./maintenance/detect-incorrect-usages.sh",
		"prettier": "prettier --write '**/*.{ts,js,json,css,scss,html}'",
		"format": "npm run prettier && npm run lintf",
		"prepare": "echo 'üç∑ Building Wikia/ad-engine' && npm run build --production",
		"preversion": "./maintenance/preversion.sh",
		"version": "./maintenance/version.sh",
		"serve": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --inline --open --watch --content-base=./examples",
		"tdd": "mocha-webpack -w --full-trace './spec/**/*.spec.ts' -r jsdom-global/register --webpack-env.test",
		"test": "mocha-webpack --full-trace './spec/**/*.spec.ts' -r jsdom-global/register --webpack-env.test",
		"test-coverage": "nyc mocha-webpack './spec/**/*.spec.ts' -r jsdom-global/register --webpack-env.test --reporter mocha-junit-reporter --reporter-options mochaFile=./spec/build/tests.xml",
		"watch": "./node_modules/webpack/bin/webpack.js --progress --colors --hide-modules --watch",
		"wdio-desktop": "BABEL_ENV=test ./node_modules/.bin/wdio wdio.desktop.conf.js",
		"wdio-mobile": "BABEL_ENV=test ./node_modules/.bin/wdio wdio.mobile.conf.js",
		"wdio-all": "npm run wdio-mobile; npm run wdio-desktop",
		"allure": "cp -R ./tests/allure-results/report/history ./tests/allure-results | true && allure generate ./tests/allure-results --output ./tests/allure-results/report --clean && allure open ./tests/allure-results/report",
		"allure-clear": "find ./tests/allure-results/ -name *attachment.json -delete; find ./tests/allure-results/ -name *testsuite.xml -delete; find ./tests/allure-results/ -name *attachment.png -delete",
		"check-types": "tsc -noEmit",
		"check-types:watch": "tsc --watch -noEmit",
		"rules:build": "tsc --project 'maintenance/tslint-rules/tsconfig.json'",
		"rules:tests": "tslint --project 'maintenance/tslint-rules/tests/tsconfig.json'",
		"rules:lint": "tslint --project 'maintenance/tslint-rules/tsconfig.json'",
		"rules:lintf": "tslint --project 'maintenance/tslint-rules/tsconfig.json' --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"*.ts": [
				"prettier --write",
				"tslint --fix",
				"./maintenance/detect-incorrect-usages.sh",
				"git add"
			],
			"*.{js,json,css,scss,html}": [
				"prettier --write",
				"git add"
			]
		},
		"ignore": [
			"**/dist/**/*",
			"maintenance/tslint-rules/tests/**/*"
		]
	}
}
