		"@wdio/cli": "5.7.10",
		"@wdio/devtools-service": "5.4.6",
		"@wdio/local-runner": "5.7.9",
		"@wdio/mocha-framework": "5.7.8",
		"@wdio/selenium-standalone-service": "5.7.8",
		"@wdio/spec-reporter": "5.7.9",
		"@wdio/sync": "5.7.9",
		"allure-commandline": "2.7.0",
		"awesome-typescript-loader": "5.2.1",
		"babel-loader": "^8.0.0",
		"babel-plugin-lodash": "^3.3.2",
		"chai": "^4.1.2",
		"chrome-remote-interface": "0.26.1",
		"concat": "1.0.3",
		"copy-webpack-plugin": "4.5.2",
		"deepmerge": "2.2.1",
		"express": "^4.16.2",
		"husky": "1.1.4",
		"js-md5": "0.7.3",
		"jsdom": "11.11.0",
		"jsdom-global": "^3.0.2",
		"json-loader": "0.5.7",
		"jwplayer-fandom": "github:wikia/jwplayer-fandom#1.6.1",
		"lint-staged": "8.0.5",
		"mini-css-extract-plugin": "0.4.0",
