		"mocha-junit-reporter": "1.23.0",
		"mocha-webpack": "2.0.0-beta.0",
		"node-sass": "4.12.0",
		"nyc": "11.4.1",
		"portfinder": "1.0.20",
		"prettier": "1.18.2",
		"rollup": "1.10.1",
		"rollup-plugin-babel": "4.3.3",
		"rollup-plugin-commonjs": "9.3.4",
		"rollup-plugin-json": "4.0.0",
		"rollup-plugin-node-resolve": "4.2.3",
		"rollup-plugin-postcss": "2.0.3",
		"rollup-plugin-serve": "1.0.1",
		"rollup-plugin-sourcemaps": "0.4.2",
		"rollup-plugin-terser": "5.0.0",
		"rollup-plugin-typescript2": "0.21.0",
		"rollup-plugin-uglify": "6.0.2",
		"sass-loader": "7.1.0",
		"selenium-standalone": "6.15.2",
		"sinon": "7.3.2",
		"string-replace-webpack-plugin": "0.1.3",
		"tscpaths": "git://github.com/bielik20/tscpaths.git#release",
		"tslint": "5.18.0",
		"tslint-ban-snippets": "2.1.0",
		"tslint-config-airbnb": "5.11.1",
		"tslint-config-prettier": "1.18.0",
		"tslint-eslint-rules": "5.4.0",
		"typescript": "3.5.2",
		"webdriverio": "5.7.9",
		"webpack": "4.35.0",
		"webpack-cli": "3.3.5",
		"webpack-dev-server": "3.7.2",
		"webpack-merge": "4.2.1"
	},
	"files": [
		"dist/",
		"lib/",
		"configs/",
		"maintenance/",
		"src/",
		".babelrc",
		"package.json",
		"webpack.config.js",
		"rollup.config.js",
		"tslint.json",
		"tslint.app.json"
	],
	"main": "dist/index.es5.js",
	"module": "dist/index.es5.js",
	"types": "dist/types/index.d.ts",
	"nyc": {
		"report-dir": "./spec/build/",
		"reporter": [
			"cobertura"
		]
	},
	"scripts": {
		"dev": "npm run serve",
		"serve": "webpack-dev-server --progress --colors --inline --open --watch --content-base=./examples --config webpack.dev.js",
		"prebuild": "rm -rf dist",
		"build": "rollup -c rollup.config.js",
		"build:examples": "webpack --progress --colors --hide-modules --config webpack.dev.js",
		"build:gp": "webpack --env.PLATFORM=gamepedia --config webpack.platforms.js",
		"build:gp-rollup": "rollup -c rollup.platforms.js",
		"watch": "npm run build -- -w",
		"ci-check": "npm run test && npm run lint",
		"lint": "tslint --project . && npm run rules:lint",
		"lintf": "tslint --project . --fix && npm run rules:lintf",
		"prettier": "prettier --write '**/*.{ts,js,json,css,scss,html}'",
		"format": "npm run prettier && npm run lintf",
		"prepare": "echo '🍷 Building Wikia/ad-engine' && npm run build --production",
		"preversion": "./maintenance/preversion.sh",
		"version": "./maintenance/version.sh",
		"test": "mocha-webpack --full-trace './spec/**/*.spec.ts' -r jsdom-global/register --webpack-config webpack.spec.js",
		"test:watch": "npm run test -- -w",
		"test-coverage": "nyc mocha-webpack './spec/**/*.spec.ts' -r jsdom-global/register --webpack-config webpack.spec.js --reporter mocha-junit-reporter --reporter-options mochaFile=./spec/build/tests.xml",
		"wdio-desktop": "BABEL_ENV=test wdio wdio.desktop.conf.js",
		"wdio-mobile": "BABEL_ENV=test wdio wdio.mobile.conf.js",
		"wdio-all": "npm run wdio-mobile; npm run wdio-desktop",
		"allure": "cp -R ./tests/allure-results/report/history ./tests/allure-results | true && allure generate ./tests/allure-results --output ./tests/allure-results/report --clean && allure open ./tests/allure-results/report",
		"allure-clear": "find ./tests/allure-results/ -name *attachment.json -delete; find ./tests/allure-results/ -name *testsuite.xml -delete; find ./tests/allure-results/ -name *attachment.png -delete",
		"check-types": "tsc -noEmit",
		"check-types:watch": "tsc --watch -noEmit",
		"check-types:selected": "npm run check-types 2> /dev/null | grep -e src/ad-engine -e @types | grep -v npm",
		"rules:build": "tsc --project 'maintenance/tslint-rules/tsconfig.json'",
		"rules:test": "tslint --project 'maintenance/tslint-rules/tests/tsconfig.json'",
		"rules:lint": "tslint --project 'maintenance/tslint-rules/tsconfig.json'",
		"rules:lintf": "tslint --project 'maintenance/tslint-rules/tsconfig.json' --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && if npm run check-types 2> /dev/null | grep -e src/ad-engine -e @types | grep -v npm; then exit 1; fi"
		}
	},
	"lint-staged": {
		"linters": {
			"*.ts": [
				"prettier --write",
				"tslint --fix",
				"git add"
			],
			"*.{js,json,css,scss,html}": [
				"prettier --write",
				"git add"
			]
		},
		"ignore": [
			"**/dist/**/*",
			"maintenance/tslint-rules/tests/**/*"
		]
	}
}
